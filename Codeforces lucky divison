u have  to print yes for lucky numbers if its is one defined as
-- if it contains 4 and 7 
--or it is divisble by those lucky numbers
 
code-
#include <bits/stdc++.h>
using namespace std;
 
int main(){
  
      int n;
      cin>>n;
      bool flag=false;
      int arr[14]={4,7,47,74,44,77,444,447,474,477,744,747,774,777};
      for(int i=0;i<14;i++){
        if(n%arr[i]==0){
                flag=true;
                break;
        }
      }
      if(flag) cout<<"YES";
      else cout<<"NO";
      return 0;
}
